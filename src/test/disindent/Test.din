use Math

double foo(ca is int, cb is int)
	for x is int, 0, <(x, ca), +(x, 1)
		for y is int, 0, <(y, cb), +(y, 1)
			foo2(use (x) as double, use (y) as double)

double foo2(a is double, b is double)
	call + with
		Math.hypot(a, b)
		call * with
			+(a, a, a, a)
			+(b, b, b, b)
		call + with
			*(a, a, a, a)
			*(b, b, b, b)
